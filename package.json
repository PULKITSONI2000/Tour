	"license": "ISC",
	"devDependencies": {
		"concurrently": "^6.1.0",
		"cross-env": "^7.0.3",
		"morgan": "^1.10.0",
		"nodemon": "^2.0.7"
	},
	"dependencies": {
		"body-parser": "^1.19.0",
		"cookie-parser": "^1.4.5",
		"cors": "^2.8.5",
		"dotenv": "^8.2.0",
		"express": "^4.17.1",
		"express-jwt": "^6.0.0",
		"express-validator": "^6.10.1",
		"jsonwebtoken": "^8.5.1",
		"mongoose": "^5.12.7",
		"uuid": "^8.3.2"
	}
