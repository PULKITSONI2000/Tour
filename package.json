{
	"name": "tour",
	"version": "1.0.0",
	"description": "A touring Website created using MERN Stack",
	"main": "server.js",
	"scripts": {
		"test": "cross-env NODE_ENV=development nodemon --no-deprecation server.js",
		"start": "node server.js",
		"client": "cd frontend && npm start",
		"dev": "concurrently \"npm run test\" \"npm run client\""
	},
